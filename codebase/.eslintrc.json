{
  "env": {
    "es6": true,
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:jest/recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    },
    "jest": {
      "version": "latest"
    }
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "plugins": ["prettier", "react"]
}
